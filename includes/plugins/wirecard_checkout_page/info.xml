        <Adminmenu>
            <Settingslink sort="1">
                <Name>Einstellungen</Name>

                <Setting type="text" initialValue="D200001" sort="10" conf="Y">
                    <Name>Customer Id</Name>
                    <Description>Wirecard Kundennummer</Description>
                    <ValueName>wirecard_checkout_page_customer_id</ValueName>
                </Setting>
                <Setting type="text" initialValue="" sort="20" conf="Y">
                    <Name>Shop Id</Name>
                    <Description>Kennung der Shopseite</Description>
                    <ValueName>wirecard_checkout_page_shop_id</ValueName>
                </Setting>
                <Setting type="text" initialValue="B8AKTPWBRMNBV455FG6M2DANE99WU2" sort="30" conf="Y">
                    <Name>Secret</Name>
                    <Description>Preshared Key zur Signierung der übertragenen Parameter</Description>
                    <ValueName>wirecard_checkout_page_secret</ValueName>
                </Setting>

                <Setting type="text" initialValue="" sort="40" conf="Y">
                    <Name>Bild Url</Name>
                    <Description>Url zu Ihrem Logo auf der Bezahlseite (vorzugsweise 95x65 Pixel)</Description>
                    <ValueName>wirecard_checkout_page_image_url</ValueName>
                </Setting>

                <Setting type="text" initialValue="" sort="50" conf="Y">
                    <Name>Service Url</Name>
                    <Description>URL Ihrer Kontakt-/Impressumsseite</Description>
                    <ValueName>wirecard_checkout_page_service_url</ValueName>
                </Setting>

                <Setting type="text" initialValue="" sort="60" conf="Y">
                    <Name>Max. Versuche</Name>
                    <Description>Anzahl der maximalen Zahlungsversuche</Description>
                    <ValueName>wirecard_checkout_page_max_retries</ValueName>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="70" conf="Y">
                    <Name>Autom. abbuchen</Name>
                    <Description>Automatisches Abbuchen der Zahlung</Description>
                    <ValueName>wirecard_checkout_page_auto_deposit</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="80" conf="Y">
                    <Name>Iframe verwenden</Name>
                    <Description>Bezahlfenster wird in einem Iframe geöffnet</Description>
                    <ValueName>wirecard_checkout_page_use_iframe</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="80" conf="Y">
                    <Name>Detaillierte Kundendaten senden</Name>
                    <Description>Detaillierte Kundendaten senden</Description>
                    <ValueName>wirecard_checkout_page_send_additional_data</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="text" initialValue="" sort="90" conf="Y">
                    <Name>Text auf der Bezahlseite</Name>
                    <Description>Text, der dem Kunden zu den Bestelldaten angezeigt wird</Description>
                    <ValueName>wirecard_checkout_page_display_text</ValueName>
                </Setting>


            </Settingslink>
        </Adminmenu>
