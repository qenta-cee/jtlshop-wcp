<?xml version='1.0' encoding="ISO-8859-1"?>
<jtlshop3plugin>
    <Name>Wirecard Checkout Page</Name>
    <Description>Wirecard CEE is a popular payment provider and available in many countries.</Description>
    <Author>Wirecard CEE</Author>
    <URL>http://www.wirecard.at/</URL>
    <XMLVersion>100</XMLVersion>
    <ShopVersion>315</ShopVersion>
    <Shop4Version>400</Shop4Version>
    <PluginID>wirecard_checkout_page</PluginID>
    <Install>
        <Version nr="100">
            <CreateDate>2017-02-28</CreateDate>
            <!--<SQL>install.sql</SQL>-->
        </Version>
        <Version nr="107">
            <CreateDate>2017-02-28</CreateDate>
            <!--<SQL>install.sql</SQL>-->
        </Version>
        <Adminmenu>
            <Settingslink sort="1">
                <Name>Einstellungen</Name>

                <Setting type="selectbox" initialValue="production" sort="5" conf="Y">
                    <Name>Konfiguration</Name>
                    <Description>Zum Testen der Integration eine vordefinierte Konfiguration auswählen. Für Produktivsysteme "Production" auswählen.</Description>
                    <ValueName>wirecard_checkout_page_configuration</ValueName>
                    <SelectboxOptions>
                        <Option value="production" sort="1">Produktion</Option>
                        <Option value="demo" sort="2">Demo</Option>
                        <Option value="test" sort="3">Test</Option>
                        <Option value="test3d" sort="4">Test 3D</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="text" initialValue="D200001" sort="10" conf="Y">
                    <Name>Customer Id</Name>
                    <Description>Wirecard Kundennummer</Description>
                    <ValueName>wirecard_checkout_page_customer_id</ValueName>
                </Setting>
                <Setting type="text" initialValue="" sort="20" conf="Y">
                    <Name>Shop Id</Name>
                    <Description>Kennung der Shopseite</Description>
                    <ValueName>wirecard_checkout_page_shop_id</ValueName>
                </Setting>
                <Setting type="text" initialValue="B8AKTPWBRMNBV455FG6M2DANE99WU2" sort="30" conf="Y">
                    <Name>Secret</Name>
                    <Description>Preshared Key zur Signierung der übertragenen Parameter</Description>
                    <ValueName>wirecard_checkout_page_secret</ValueName>
                </Setting>

                <Setting type="text" initialValue="" sort="40" conf="Y">
                    <Name>Bild Url</Name>
                    <Description>Url zu Ihrem Logo auf der Bezahlseite (vorzugsweise 95x65 Pixel)</Description>
                    <ValueName>wirecard_checkout_page_image_url</ValueName>
                </Setting>

                <Setting type="text" initialValue="" sort="50" conf="Y">
                    <Name>Service Url</Name>
                    <Description>URL Ihrer Kontakt-/Impressumsseite</Description>
                    <ValueName>wirecard_checkout_page_service_url</ValueName>
                </Setting>

                <Setting type="text" initialValue="-1" sort="60" conf="Y">
                    <Name>Max. Versuche</Name>
                    <Description>Anzahl der maximalen Zahlungsversuche</Description>
                    <ValueName>wirecard_checkout_page_max_retries</ValueName>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="70" conf="Y">
                    <Name>Autom. abbuchen</Name>
                    <Description>Automatisches Abbuchen der Zahlung</Description>
                    <ValueName>wirecard_checkout_page_auto_deposit</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="80" conf="Y">
                    <Name>Iframe verwenden</Name>
                    <Description>Bezahlfenster wird in einem Iframe geöffnet</Description>
                    <ValueName>wirecard_checkout_page_use_iframe</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="80" conf="Y">
                    <Name>Detaillierte Kundendaten senden</Name>
                    <Description>Detaillierte Kundendaten senden</Description>
                    <ValueName>wirecard_checkout_page_send_additional_data</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="85" conf="Y">
                    <Name>Warenkorbdaten des Konsumenten mitsenden</Name>
                    <Description>Weiterleitung des Warenkorbs des Kunden an den Finanzdienstleister.</Description>
                    <ValueName>wirecard_checkout_page_send_basket</ValueName>
                    <SelectboxOptions>
                        <Option value="1" sort="1">On</Option>
                        <Option value="0" sort="2">Off</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="text" initialValue="" sort="90" conf="Y">
                    <Name>Text auf der Bezahlseite</Name>
                    <Description>Text, der dem Kunden zu den Bestelldaten angezeigt wird</Description>
                    <ValueName>wirecard_checkout_page_display_text</ValueName>
                </Setting>

                <Setting type="text" initialValue="Y" sort="99" conf="N">
                    <Name>Kauf auf Rechnung / Raten Einstellungen</Name>
                    <Description />
                    <ValueName>wirecard_checkout_page_installment_title</ValueName>
                </Setting>

                <Setting type="selectbox" initialValue="wirecard" sort="100" conf="Y">
                    <Name>Provider für Rechnung</Name>
                    <ValueName>wirecard_checkout_page_invoice_provider</ValueName>
                    <SelectboxOptions>
                        <Option value="wirecard" sort="1">Wirecard</Option>
                        <Option value="ratepay" sort="2">RatePay</Option>
                        <Option value="payolution" sort="3">payolution</Option>
                    </SelectboxOptions>
                </Setting>


                <Setting type="selectbox" initialValue="ratepay" sort="110" conf="Y">
                    <Name>Provider für Kauf auf Raten</Name>
                    <ValueName>wirecard_checkout_page_installment_provider</ValueName>
                    <SelectboxOptions>
                        <Option value="ratepay" sort="1">RatePay</Option>
                        <Option value="payolution" sort="2">payolution</Option>
                    </SelectboxOptions>
                </Setting>

                <Setting type="selectbox" initialValue="0" sort="120" conf="Y">
                    <Name>Payolution Nutzungsbedingungen</Name>
                    <Description>Kunden müssen die Nutzungsbedingungen von payolution während des Bezahlprozesses akzeptieren.</Description>
                    <ValueName>wirecard_checkout_page_payolution_terms</ValueName>
                    <SelectboxOptions>
                        <Option value="0" sort="1">Off</Option>
                        <Option value="1" sort="2">On</Option>
                    </SelectboxOptions>
                </Setting>
                <Setting type="text" initialValue="" sort="121" conf="Y">
                    <Name>payolution mID</Name>
                    <Description>payolution-Händler-ID, Nicht base64 kodiert.</Description>
                    <ValueName>wirecard_checkout_page_payolution_mid</ValueName>
                </Setting>


            </Settingslink>
        </Adminmenu>
        <Hooks>
            <Hook id="140">140_HOOK_SMARTY_OUTPUTFILTER.php</Hook>
            <Hook id="19">payment_selection.php</Hook>
            <Hook id="144">iframebreakout.php</Hook>
        </Hooks>

